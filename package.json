{
  "name": "object-take",
  "version": "0.1.3",
  "description": "A really small library, almost 150 bytes minified and gzipped, to take value from object using a string for path expression or returns a placeholder if value is undefined or unreachable.",
  "cdn": "dist/object-take.umd.min.js",
  "main": "dist/object-take.js",
  "types": "index.d.ts",
  "unpkg": "dist/object-take.umd.min.js",
  "module": "dist/object-take.esm.js",
  "jsdelivr": "dist/object-take.umd.min.js",
  "umd:main": "dist/object-take.umd.js",
  "exports": {
    ".": [
      {
        "import": "./dist/object-take.mjs",
        "default": "./dist/object-take.js",
        "require": "./dist/object-take.cjs"
      },
      "./dist/object-take.js"
    ]
  },
  "files": [
    "dist/",
    "types/"
  ],
  "scripts": {
    "test": "pnpm run test:compile && pnpm run test:unit",
    "test:unit": "ava index.spec.js",
    "test:compile": "tsc -p ./tsconfig.test.json",
    "build:compile": "tsc -p ./tsconfig.build.json",
    "build:bundle": "rollup --config rollup.config.js",
    "build": "pnpm run build:compile && pnpm run build:bundle"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VitorLuizC/object-take.git"
  },
  "author": {
    "url": "https://vitorluizc.github.io/",
    "name": "Vitor Luiz Cavalcanti",
    "email": "vitorluizc@outlook.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/VitorLuizC/object-take/issues"
  },
  "homepage": "https://github.com/VitorLuizC/object-take#readme",
  "devDependencies": {
    "ava": "^3.15.0",
    "rollup": "^2.60.0",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.5.2"
  }
}
